  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethercards/ec-util"
  },
  "keywords": [
    "ec-util"
  ],
  "bugs": {
    "url": "https://github.com/ethercards/ec-util/issues"
  },
  "homepage": "https://github.com/ethercards/ec-util#readme",
  "devDependencies": {
    "@types/chai": "^4.2.17",
    "@types/mocha": "^5.2.5",
    "@types/node": "^14.14.31",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "bitwise": "^2.1.0",
    "browserify": "^16.2.3",
